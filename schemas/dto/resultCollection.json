{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Collection of result DTO",
  "type": "object",
  "properties": {
    "results": {
      "description": "List of results",
      "type": "array",
      "id": "resultList",
      "items": {
        "type": "object",
        "$ref": "result.json"
      }
    },
    "totalRecords": {
      "description": "Number of results in the list",
      "type": "integer"
    }
  },
  "required": [
    "results",
    "totalRecords"
  ]
}
